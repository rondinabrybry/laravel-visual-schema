{
    "name": "brybry/laravel-visual-schema",
    "description": "A drag-and-drop visual database designer for Laravel. Shared hosting compatible with no build process required.",
    "type": "library",
    "keywords": ["laravel", "database", "schema", "visual", "designer", "erd", "shared-hosting", "alpine", "fabric", "no-build"],
    "license": "MIT",
    "authors": [
        {
            "name": "BryBry",
            "email": "your-email@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "BryBry\\LaravelVisualSchema\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BryBry\\LaravelVisualSchema\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BryBry\\LaravelVisualSchema\\LaravelVisualSchemaServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}